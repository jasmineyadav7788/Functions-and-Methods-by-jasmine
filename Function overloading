public class OverloadingExample {
  
    // Overloaded function with a single integer parameter
    public void display(int num) {
        System.out.println("Displaying integer: " + num);
    }
  
    // Overloaded function with a single double parameter
    public void display(double num) {
        System.out.println("Displaying double: " + num);
    }
  
    // Overloaded function with two integer parameters
    public void display(int num1, int num2) {
        System.out.println("Displaying integers: " + num1 + " and " + num2);
    }
  
    public static void main(String[] args) {
        OverloadingExample example = new OverloadingExample();
        
        // Call the overloaded functions
        example.display(10);              // Calls the display(int num) method
        example.display(2.5);             // Calls the display(double num) method
        example.display(3, 4);            // Calls the display(int num1, int num2) method
    }
}
